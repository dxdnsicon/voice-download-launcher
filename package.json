{
  "name": "tdvoice-launcher",
  "version": "2.2.0",
  "description": "td视频下载转音频工具（自用）",
  "scripts": {
    "dev": "tsc -w & babel src  --extensions '.js' --out-dir es -w",
    "install:chrome": "node node_modules/puppeteer-core/install.js",
    "start": "pm2-runtime es/index.js",
    "start:dev": "node es/index.js",
    "debug": "node es/debug.js",
    "pm2": "pm2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dxdnsicon/voice-download-launcher"
  },
  "bin": {
    "tdload": "es/index.js"
  },
  "files": [
    "es",
    ".env"
  ],
  "author": "dxdnsicon<dxdnsicon@github.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "7.8.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "dotenv": "^16.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "puppeteer-core": "10.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.1.0",
    "babel-eslint": "10.1.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^3.0.9",
    "jest": "^27.5.1",
    "lint-staged": "^9.4.3",
